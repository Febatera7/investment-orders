{
	"compilerOptions": {
		"target": "es2020",
		"module": "commonjs",
		"lib": ["es6"],
		"allowJs": true,
		"outDir": "./dist",
		"rootDir": "./src",
		"removeComments": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"strict": false,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src/**/*", ".sequelizerc"]
}
